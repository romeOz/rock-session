{
    "name": "romeoz/rock-session",
    "description": "A simple Session library for PHP",
    "keywords": ["session", "memory", "redis", "memcached", "mongodb", "mysql", "postgres"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-session/issues",
        "source": "https://github.com/romeOz/rock-session"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-events" : "0.11.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7.0",
        "romeoz/rock-cache": "0.15.*",
        "romeoz/rock-mongodb": "0.15.*",
        "romeoz/rock-db": "0.15.*"
    },
    "suggest": {
        "romeoz/rock-cache": "required for MemorySession",
        "romeoz/rock-mongodb": "required for MongoSession",
        "romeoz/rock-db": "required for DbSession"
    },
    "autoload": {
        "psr-4": {
            "rock\\session\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    }
}